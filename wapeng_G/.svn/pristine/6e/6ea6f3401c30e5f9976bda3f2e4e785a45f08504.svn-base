#import "HeaderKindView.h"

@implementation HeaderKindView

- (id)initWithFrame:(CGRect)frame
{
    self = [super initWithFrame:frame];
    if (self) {
        
        self.backgroundColor = [UIColor grayColor];
        self.sectionLbl = [[UILabel alloc]initWithFrame:CGRectMake(20, 0, frame.size.width - 40, frame.size.height)];
        self.sectionLbl.text = @"九宫格头部标题";
//        self.backgroundColor = [UIColor greenColor];
        self.sectionLbl.backgroundColor = [UIColor grayColor];
        [self addSubview:self.sectionLbl];
        
    }
    return self;
}

